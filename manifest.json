{
	"manifest_version": 3,
	"name": "OTS Plus",
	"version": "1.0",
	"content_scripts": [
		{
			"matches": ["http://192.168.1.148:8081/hkots/*"],
			"js": ["content.js"]
		}
	],
	"permissions": ["activeTab","storage","contextMenus"],
	"icons": {
		"500": "icons/icon_500x500.png"
	},
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  }
}
